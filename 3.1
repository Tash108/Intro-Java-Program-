import java.util.Scanner;
public class nameArray{
  
	public static void main(String[] args) {
		// TODO Auto-generated method stub

		String names[]=new String[5];//creates string, holds 5 elements
		names[0]="John"; //populate the array
		names[1]="Joanne";
		names[2]="Jannie";
		names[3]="Rose";
		names[4]="Sweety";
		for (int i=0;i<5;i++) {
			System.out.println(names[i]);// loop through the array
		}
	}

}
